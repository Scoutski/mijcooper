<!DOCTYPE html>
<html>
<head>
  <title>Mijcooper</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

	<div class="container">
		<div class="nav navbar-nav">
			<%== intellinav %>
		</div>

		<% if flash[:notice].present? %>
			<h2><%= flash[:notice] %></h2>
		<% end %>
		
		<%= yield %>
	</div>

</body>
</html>
